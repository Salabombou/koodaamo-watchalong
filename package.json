{
  "name": "koodaamo-watchalong",
  "productName": "koodaamo-watchalong",
  "version": "1.1.11",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Salabombou/koodaamo-watchalong.git"
  },
  "description": "An Electron app for synchronizing video playback with friends.",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "start:2": "electron . --user-data-dir=./.tmp/client2",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf dist release",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" webpack --config webpack.build.main.ts",
    "build:renderer": "cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" webpack --config webpack.build.renderer.ts",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.salabombou.koodaamo-watchalong",
    "productName": "koodaamo-watchalong",
    "compression": "maximum",
    "directories": {
      "output": "release"
    },
    "files": [
      "package.json",
      ".webpack/**/*",
      "!node_modules/**/*",
      "node_modules/utp-native/**/*"
    ],
    "extraResources": [],
    "win": {
      "target": [
        "nsis"
      ],
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/bin/win32/x64/ffprobe.exe",
          "to": "ffprobe.exe"
        },
        {
          "from": "node_modules/ffmpeg-static/ffmpeg.exe",
          "to": "ffmpeg.exe"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/bin/darwin/x64/ffprobe",
          "to": "ffprobe"
        },
        {
          "from": "node_modules/ffmpeg-static/ffmpeg",
          "to": "ffmpeg"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/bin/linux/x64/ffprobe",
          "to": "ffprobe"
        },
        {
          "from": "node_modules/ffmpeg-static/ffmpeg",
          "to": "ffmpeg"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Salabombou",
        "repo": "koodaamo-watchalong"
      }
    ]
  },
  "keywords": [
    "electron",
    "watchalong",
    "video",
    "sync"
  ],
  "author": {
    "name": "Salabombou",
    "email": "salabombo@outlook.com"
  },
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bitdisaster/exe-icon-extractor",
      "bufferutil",
      "electron",
      "electron-winstaller",
      "ffmpeg-static",
      "node-datachannel",
      "utf-8-validate",
      "utp-native"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-wix": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^1.0.0",
    "@eslint/markdown": "^7.5.1",
    "@reforged/maker-appimage": "^5.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.2",
    "@types/range-parser": "^1.2.7",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/webtorrent": "^0.110.1",
    "@vercel/webpack-asset-relocator-loader": "^1.10.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.3",
    "daisyui": "^5.5.18",
    "electron": "40.1.0",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^17.3.0",
    "html-webpack-plugin": "^5.6.6",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "3.8.1",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "wait-on": "^9.0.4",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "dependencies": {
    "@achingbrain/nat-port-mapper": "^4.0.5",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@types/video.js": "^7.3.58",
    "axios": "^1.13.5",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "fs-extra": "^11.3.3",
    "jiti": "^2.6.1",
    "node-datachannel": "^0.32.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "uuid": "^13.0.0",
    "video.js": "^8.23.6",
    "webtorrent": "^2.8.5",
    "winston": "^3.19.0"
  }
}
